<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bocs.mapper.RoomStatusMapper">
    <select id="queryRoomStatusList" resultType="RoomStatus" useCache="true">
        select * from RoomStatus
    </select>

    <select id="queryRoomStatusById" resultType="String">
        select * from RoomStatus where roomStatusId=#{roomStatusId}
    </select>


</mapper>